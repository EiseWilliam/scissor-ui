{
  "name": "vite-react-boilerplate",
  "author": "Ricardo Valdovinos <ricardovaldovinos1996@gmail.com>",
  "description": "A production ready, batteries included starter template for Vite + React projects",
  "keywords": [
    "vite",
    "react",
    "boilerplate",
    "starter",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RicardoValdovinos/vite-react-boilerplate"
  },
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "git init && husky install && npx playwright install",
    "format": "prettier src/**/*.{ts,tsx} --write",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "dev": "vite",
    "storybook": "storybook dev -p 6006",
    "storybook:setup": "node node_modules/@storybook/addon-styling/bin/postinstall.js",
    "storybook:build": "storybook build",
    "test": "vitest run src/ && playwright test",
    "test:unit": "vitest src/",
    "test:unit:coverage": "vitest --coverage src/",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@nivo/bar": "^0.85.1",
    "@nivo/core": "^0.85.1",
    "@nivo/line": "^0.85.1",
    "@nivo/pie": "^0.85.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@sinclair/typebox": "^0.32.15",
    "@south-paw/react-vector-maps": "^3.2.0",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-router": "^1.20.0",
    "@tanstack/react-table": "^8.13.2",
    "@tanstack/router-vite-plugin": "^1.20.0",
    "@tremor/react": "^3.14.1",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "framer-motion": "^11.0.14",
    "jquery": "^3.7.1",
    "lucide-react": "^0.358.0",
    "npm": "^10.5.0",
    "npm-check-updates": "^16.14.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-inlinesvg": "^4.1.3",
    "recharts": "^2.12.3",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "tremor": "^0.0.1",
    "vaul": "^0.9.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@hookform/devtools": "^4.3.1",
    "@tanstack/react-query-devtools": "^5.28.4",
    "@tanstack/react-table-devtools": "^8.13.2",
    "@tanstack/router-devtools": "1.20.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/bun": "^1.0.8",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "1.4.0",
